<MudDialog>
    <DialogContent>
        <MudText>@dialogText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Filled" Size="Size.Small" Color="Color.Error" OnClick="Cancel">Ne</MudButton>
        <MudButton Variant="Variant.Filled" Size="Size.Small" Color="Color.Success" OnClick="(async ()=>await Submit())">Da</MudButton>
    </DialogActions>
</MudDialog>
